
<fontconfig>

	<!--github-->

	<match>
		<test name='family'>
			<string>sfmono-regular</string>
		</test>
		<edit name='family'>
			<string>dmca sans serif</string>
		</edit>
	</match>

	<!--gitlab-->

	<match>
		<test name='family'>
			<string>blinkmacsystemfont</string>
		</test>
		<edit name='family'>
			<string>dmca sans serif</string>
		</edit>
	</match>

	<match>
		<test name='family'>
			<string>menlo</string>
		</test>
		<edit name='family'>
			<string>dmca sans serif</string>
		</edit>
	</match>

	<!--google-->

	<match>
		<test name='family'>
			<string>arial</string>
		</test>
		<edit name='family' binding='same'>
			<string>roboto mono</string>
		</edit>
	</match>

	<!--times-->

	<match>
		<test name='family'>
			<string>times new roman</string>
		</test>
		<edit name='family' binding='same'>
			<string>roboto slab</string>
		</edit>
	</match>

</fontconfig>
