
<fontconfig>

	<alias>
		<family>monospace</family>
		<prefer>
			<family>dmca sans serif</family>
		</prefer>
	</alias>

	<alias>
		<family>sans-serif</family>
		<prefer>
			<family>roboto</family>
		</prefer>
	</alias>

	<alias>
		<family>serif</family>
		<prefer>
			<family>freeserif</family>
		</prefer>
	</alias>

	<alias>
		<family>system-ui</family>
		<prefer>
			<family>dmca sans serif</family>
		</prefer>
	</alias>

	<!--gitlab-->

	<alias>
		<family>menlo</family>
		<prefer>
			<family>monospace</family>
		</prefer>
	</alias>

	<!--rutracker-->

	<alias>
		<family>verdana</family>
		<prefer>
			<family>hack</family>
		</prefer>
	</alias>

	<!--vscode-->

	<alias>
		<family>ubuntu</family>
		<prefer>
			<family>monospace</family>
		</prefer>
	</alias>

</fontconfig>
