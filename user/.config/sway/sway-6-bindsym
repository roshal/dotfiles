
bindsym --to-code $-modifier+q       kill
bindsym --to-code $-modifier+q+shift kill

##### 6.1 executing applications

bindsym $-modifier+return exec termite

bindsym --to-code $-modifier+d exec wofi
bindsym --to-code $-modifier+i exec swaylock --image /-/pictures/waves.jpg
bindsym --to-code $-modifier+n exec thunar
bindsym --to-code $-modifier+o exec swaymsg "output $-echo-output-focused disable, output $-echo-output-suspend enable"
bindsym --to-code $-modifier+u exec swaylock --color ffffff

bindsym --to-code $-modifier+u+shift exec swaylock --color 0000ff

bindsym --to-code $-modifier+shift+e exec swaynag $-swaynag

bindsym            $-key-p       exec $-grim-crop $-grim-path
bindsym            $-key-p+ctrl  exec $-grim-view $-grim-path
bindsym            $-key-p+shift exec $-grim-full $-grim-path
bindsym $-modifier+$-key-p       exec $-grim-crop $-grim-copy
bindsym $-modifier+$-key-p+ctrl  exec $-grim-view $-grim-copy
bindsym $-modifier+$-key-p+shift exec $-grim-full $-grim-copy

bindsym $-modifier+shift+s exec $-grim-slurp

