##### 6.8 changing workspaces or moving to workspaces

bindsym $-modifier+tab workspace back_and_forth
bindsym $-modifier+caps_lock workspace back_and_forth

bindsym $-modifier+$-way-n      workspace next_on_output
bindsym $-modifier+$-way-r+ctrl workspace next_on_output
bindsym $-modifier+$-way-r+mod1 workspace next_on_output

bindsym $-modifier+$-way-p      workspace prev_on_output
bindsym $-modifier+$-way-l+ctrl workspace prev_on_output
bindsym $-modifier+$-way-l+mod1 workspace prev_on_output

bindsym --to-code $-modifier+x workspace next_on_output
bindsym --to-code $-modifier+z workspace prev_on_output

bindsym --to-code $-modifier+$-key-0 workspace $-workspace-0
bindsym --to-code $-modifier+$-key-1 workspace $-workspace-1
bindsym --to-code $-modifier+$-key-2 workspace $-workspace-2
bindsym --to-code $-modifier+$-key-3 workspace $-workspace-3
bindsym --to-code $-modifier+$-key-4 workspace $-workspace-4
bindsym --to-code $-modifier+$-key-5 workspace $-workspace-5
bindsym --to-code $-modifier+$-key-6 workspace $-workspace-6
bindsym --to-code $-modifier+$-key-7 workspace $-workspace-7
bindsym --to-code $-modifier+$-key-8 workspace $-workspace-8
bindsym --to-code $-modifier+$-key-9 workspace $-workspace-9

bindsym $-modifier+$-key-i workspace $-workspace-6
bindsym $-modifier+$-key-h workspace $-workspace-7
bindsym $-modifier+$-key-d workspace $-workspace-8
bindsym $-modifier+$-key-e workspace $-workspace-9

bindsym $-modifier+$-key-i+shift move container to $-workspace-6
bindsym $-modifier+$-key-h+shift move container to $-workspace-7
bindsym $-modifier+$-key-d+shift move container to $-workspace-8
bindsym $-modifier+$-key-e+shift move container to $-workspace-9

bindsym $-modifier+$-key-i+mod1 workspace $-workspace-1
bindsym $-modifier+$-key-h+mod1 workspace $-workspace-2
bindsym $-modifier+$-key-d+mod1 workspace $-workspace-3
bindsym $-modifier+$-key-e+mod1 workspace $-workspace-4

bindsym $-modifier+$-key-i+mod1+shift move container to $-workspace-1
bindsym $-modifier+$-key-h+mod1+shift move container to $-workspace-2
bindsym $-modifier+$-key-d+mod1+shift move container to $-workspace-3
bindsym $-modifier+$-key-e+mod1+shift move container to $-workspace-4

